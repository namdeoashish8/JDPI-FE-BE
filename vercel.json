{
  "version": 2,
  "builds": [
    {
      "src": "JDPI-BE/index.js",
      "use": "@vercel/node"
    }
  ,
   {
      "src": "JDPI-FE/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "JDPI-FE/build" }
    }
    
  ],
  "routes": [
    { "src": "/JDPI-BE/(.*)", "dest": "JDPI-BE/index.js" 
    }, 
    { "src": "/(.*)", "dest": "JDPI-FE/build/index.html"}
  ]
}
